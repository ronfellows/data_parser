        <div class="row">
            
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <h1 class="text-center">Copy Data below</h1>
                <textarea ng-model="dataInput" ng-change="newData()" id="data-in" class="form-control" rows="3" required="required"></textarea>
            </div>
            
            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"></div>
            
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                
                <button type="button" class="btn btn-primary data-button">Convert to XML</button>
                <button type="button" class="btn btn-primary data-button">Convert to CSV</button>
                <button type="button" class="btn btn-primary data-button">Convert to JSON</button>
                
                
            </div>
            
            <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"></div>

        </div>
<% include footer %>